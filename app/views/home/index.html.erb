<!--MAPS-->
<div id="map" style="margin:auto;max-width:800px;height:450px;box-shadow: 0 2px 4px 0 rgba(0, 0, 0, 0.4)"></div>

<script>
function myMap() {
  var myCenter = new google.maps.LatLng(48.8305229,2.374795899999981);
  var mapCanvas = document.getElementById("map");
  var mapOptions = {center: myCenter, zoom: 16};
  var map = new google.maps.Map(mapCanvas, mapOptions);
  var marker = new google.maps.Marker({position:myCenter});
  marker.setMap(map);

// Responsive Map : Center the marker according the dimensions of the map window
google.maps.event.addDomListener(window, "resize", function() {
   var center = map.getCenter();
   google.maps.event.trigger(map, "resize");
   map.setCenter(center); 
});
}
</script>

<script src="http://maps.googleapis.com/maps/api/js?key=AIzaSyDj5DFWGBgTDMFs9WE3ihS-PCKF6hkacmk&callback=myMap"></script>




